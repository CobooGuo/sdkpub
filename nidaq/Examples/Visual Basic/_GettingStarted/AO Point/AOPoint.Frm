End Sub

Private Sub CWSlide1_PointerValueChanged(ByVal Pointer As Long, Value As Variant)
    UpdateAnalogOut
End Sub

Private Sub UpdateAnalogOut()
    CWAOPoint1.SingleWrite CWSlide1.Value
    ' make sure you uncheck the Perform Analog Input box if you do
    ' not have any analog input channels, otherwise connect analog
    ' output 0 to analog input 1
    If PerformAI.Value = vbChecked Then
        Dim data As Variant
        CWAIPoint1.SingleRead data
        CWGraph1.ChartY data
    End If
End Sub
