{\plain\f13\fs20\b Sub vb_err (vi As Long, err_status As Long)\par 
\par 
Dim inst_err \tab \tab As Long\par 
Dim err_message \tab As String * 250\par 
Dim retStatus \tab \tab As Long\par 
\par 
Dim nl\par 
nl = Chr(10)\par 
\par 
If VI_SUCCESS > err_status Then\par 
\par 
}{\plain\f13\fs20     'Send a device clear to ensure communication with 'the instrument.}\par 
{\plain\f13\fs20\b     retStatus = hp871x_dcl(vi)\par 
    \par 
    If (hp871x_INSTR_ERROR_DETECTED == err_status) Then\par 
            }{\plain\f13\fs20 'query the instrument for the error}\par 
{\plain\f13\fs20\b \tab retStatus = hp871x_error_query(vi, inst_err, err_message)\par 
            msg = "CHECK :Instrument Error :" & inst_err & nl & "Error Message = " & err_message\par 
\tab MsgBox msg\par 
    Else\par 
            }{\plain\f13\fs20 'get the driver error message}\par 
{\plain\f13\fs20\b \tab retStatus = hp871x_error_message(vi, err_status, err_message)\par 
\tab msg = "CHECK :Driver Error :" & errStatus & nl & "Error Message = " & err_message\par 
\tab MsgBox msg\par 
    End If\par 
End If\par 
\par 
}{\plain\f13\fs20 'retStatus=hp871x_reset(vi);\par 
'retStatus=hp871x_close(vi);\par 
}\par 
{\plain\f13\fs20\b End Sub\par 
\par 
